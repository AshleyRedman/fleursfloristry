{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "framework": "nextjs",
    "installCommand": "pnpm install --no-frozen-lockfile",
    "buildCommand": "next build",
    "devCommand": "next dev --port $PORT",
    "outputDirectory": ".next",
    "cleanUrls": true,
    "public": false,
    "trailingSlash": false,
    "regions": ["lhr1"]
}
